{
  "schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "rootDir": ".",
    "jsx": "react-jsx",
    "noImplicitAny": false,
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "incremental": true,
    "isolatedModules": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "sourceMap": false,
    "noEmit": true,
    "typeRoots": [
      "./node_modules/@types"
    ],
  },
  "exclude": [
    "node_modules"
  ],
}